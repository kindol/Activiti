        <activiti:formProperty id="name" name="申请人名称" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="submitTime" name="提交时间" type="date" datePattern="yyyy-MM-dd" required="true"></activiti:formProperty>
        <activiti:formProperty id="submitType" name="确认申请" type="string" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="sid-82175166-FF74-4DD1-8142-1390C8E59F3A" sourceRef="startEvent" targetRef="submitForm"></sequenceFlow>
    <exclusiveGateway id="decideSubmit" name="提交OR取消"></exclusiveGateway>
    <sequenceFlow id="sid-E418BA9F-140E-4A9C-97A8-82C2BB7D6747" sourceRef="submitForm" targetRef="decideSubmit"></sequenceFlow>
    <userTask id="tl_approve" name="主管审批">
      <extensionElements>
        <activiti:formProperty id="tlApprove" name="主管审批结果" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="tlMessage" name="主管备注" type="string" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="decideTLApprove" name="主管审批校验"></exclusiveGateway>
    <endEvent id="endEventCancel" name="取消"></endEvent>
    <userTask id="hr_approve" name="人事审批">
      <extensionElements>
        <activiti:formProperty id="hrApprove" name="人事审批结果" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="hrMessage" name="人事审批备注" type="string" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="decideHRApprove" name="人事审批校验"></exclusiveGateway>
    <sequenceFlow id="sid-4C64760F-BD32-447A-B31B-FD3841AD095F" sourceRef="hr_approve" targetRef="decideHRApprove"></sequenceFlow>
    <endEvent id="endEvent" name="结束"></endEvent>
    <sequenceFlow id="sid-43DBEA4D-AF37-4193-93A1-1EB6923B87B7" sourceRef="decideSubmit" targetRef="endEventCancel">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${submitType == "n" || submitType == "N"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sid-A3EA6B67-9902-48A3-830A-D6545742477D" sourceRef="decideTLApprove" targetRef="submitForm">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${tlApprove== "n" || tlApprove== "N"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sid-9BB1E567-44FF-4AE8-BDD9-717B8F768180" sourceRef="decideHRApprove" targetRef="endEvent">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${hrApprove== "y" || hrApprove== "Y"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sid-D358BEC5-E492-4C6C-AC0A-0849602BEDBA" sourceRef="decideHRApprove" targetRef="submitForm">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${hrApprove== "n" || hrApprove== "N"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sid-026EFA95-021A-44CA-BBD1-1DC7EB63A0EB" sourceRef="decideSubmit" targetRef="tl_approve">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${submitType == "y" || submitType == "Y"}]]></conditionExpression>
